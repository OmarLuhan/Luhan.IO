<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
      <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
      <PackageId>Luhan.IO</PackageId>
      <Version>1.0.1</Version>
      <Authors>Omar Luhan</Authors>
    <Company>Luhan Techlonolgy</Company>
    <Description>Una biblioteca sencilla para imprimir y registrar mensajes con color en consola.</Description>
    <PackageTags>console log print color</PackageTags>
    <RepositoryUrl>https://github.com/OmarLuhan/Luhan.IO.git</RepositoryUrl>
      <RepositoryType>git</RepositoryType>
  </PropertyGroup>
    <ItemGroup>
        <None Include="GlobalUsings.cs" Pack="true" PackagePath="buildTransitive\" />
    </ItemGroup>

</Project>
